#include <bits/stdc++.h>

using namespace std;

char closestVowel(char c)
{
    char ret = 'a';
    if(abs(c-'e') < abs(c - ret))
    {
        ret = 'e';
    }
    if(abs(c-'i') < abs(c - ret))
    {
        ret = 'i';
    }
    if(abs(c-'o') < abs(c - ret))
    {
        ret = 'o';
    }
    if(abs(c-'u') < abs(c - ret))
    {
        ret = 'u';
    }
    return ret;
}

int main() {
    string s;
    cin >> s;
    int len = s.length();
    string out = "";
    for(int i = 0; i < len; i++)
    {
        out += s[i];
        if(closestVowel(s[i])==s[i])
        {
            continue;
        }
        out += closestVowel(s[i]);
        if(s[i]=='z')
        {
            out += 'z';
        }
        else
        {
            if(closestVowel(s[i]+1)==(char)(s[i]+1))
            {
                out += (char)(s[i]+2);
            }
            else 
            {
                out += (char)(s[i]+1);
            }
        }
    }
    cout << out << "\n";
    return 0;
}
